<h1>Menu</h1>


<% @menu_items.each do |menu_item| %>
  <div class="col-md-4">
    <div class="well menu-item">
      <h2><a href="menu_items/<%= menu_item.id %>"><%= menu_item.name %></a></h2>
      <p><%= number_to_currency(menu_item.price) %></p>
      <h5><%= menu_item.description %></h5>
      <h4><%= menu_item.category %></h4>
    </div>
  </div>
<% end %>