function calculateTip() {
  var tip = document.getElementById("tip").value;
  var cart = document.getElementById("cart").innerHTML;
  var cartAmt = Number(cart);
  var onlyTip = (cartAmt * tip);
  var totalAndTip = cartAmt + onlyTip;
  document.getElementById("showTotal").innerHTML = "$" + totalAndTip.toFixed(2);
}